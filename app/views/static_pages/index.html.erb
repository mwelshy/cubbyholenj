							<div class="col-md-6">
								<%= form_for Email.new do |f| %>
									<%= f.label :fname, 'First Name' %><br>
									<%= f.text_field :fname, :placeholder => "First Name", :class => 'form-control' %><br>
							</div>
							<div class="col-md-6">
									<%= f.label :lname, 'Last Name' %><br>
									<%= f.text_field :lname, :placeholder => "Last Name", :class => 'form-control' %><br>
								</div>
						</div>

									<%= f.label :email, 'Email' %><br>
